{
  "name": "@brmorillo/utils",
  "version": "8.0.18",
  "description": "Public utility library",
  "author": "Bruno Morillo",
  "license": "MIT",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "npm run changelog && git add CHANGELOG.md",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
    "fix": "npm run lint:fix && npm run format:fix",
    "pub": "npm run fix && npm run build && npm publish --access public"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sapphire/snowflake": "^3.5.5",
    "bcryptjs": "^2.4.3",
    "ua-parser-js": "^2.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "luxon": "^3.5.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "utility",
    "collection",
    "function",
    "math",
    "object",
    "string",
    "util",
    "utils",
    "typescript",
    "library",
    "uuid",
    "validation",
    "array"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brmorillo/util.git"
  }
}
